@inproceedings{MPS94,
  author = "O. Maler and A. Pnueli and J. Sifakis",
  title = "On the Synthesis of Discrete Controllers for Timed
      Systems",
     task = {2.2},
     file = {syntc.ps.Z,Discr/last.ps},
     abstract = {This paper is a contribution to the study of
  automatic program synthesis by means of finding a winning strategy for
  an infinite game. There are two novel features in this work. First,
  for the case of discrete systems, where both the specification and the
  program consist of finite $\o$-automata, we give a radically-simpler
  formulation of the problem and of its solution, without using the
  heavy machinery of automata on trees.  Secondly we extend these
  results to timed automata, paving the way for the application of
  program synthesis techniques to the construction of real-time embedded
  systems from their specifications.},
          booktitle = {Proceedings of STACS'95},
          editor = {E.W.~Mayr and C.~Puech},
