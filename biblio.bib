@techreport{TPVS:design,
         author = "A. Pnueli",
         title = "Requirements for {\sc tpvs}:\\
A Temporal Prototype Verification System",
         INSTITUTION = "The Weizmann Institute",
     task = {1.2},
     abstract = {In this requirement and initial design document,
we describe the system {\sc tpvs}, a temporal prototype verification system.
{\sc tpvs}\ is designed to serve as a powerful front-end to the high-order
prover {\sc pvs}. It is intended that all temporal reasoning, creation and
identification of intermediate assertions and ranking functions, and
invocation of temporal verification rules be conducted on the {\sc tpvs}\
level. {\sc tpvs}\ has a limited reasoning capacity of its own which covers
boolean and restricted integer reasoning (Pressburger arithmetic).
Simulated reasoning with this restricted capacity indicates that about
95\% of the typically encountered verification conditions will be
resolved on the {\sc tpvs}\ level, and only few remaining formulas will be
transferred to the heavy reasoner {\sc pvs}.},
     file = "TPVS-design.ps.Z",
         year = 1993
}


@techreport{TPVS:progress,
         author = "A. Blinchevsky and B. Liberman and I. Usvyatsky",
         title = "{\sc tpvs}: Documentation and Progress Report",
         INSTITUTION = "The Weizmann Institute",
     task = {1.2},
     abstract = {This report presents documentation, detailed
design, and progress report for the work done in the period Dec. 93 --
May, 94 on the {\sc tpvs} project. Work in this period concentrated on
the construction of a compiler for the programming language {\sc spl}
and for temporal formulas, and on an algorithm for bringing a
verification condition, which is a first-order formula, into a normal
form, applying a set of simplification rewriting rules. The two
compilers have to be written in the Emacs environment. The report
contains the Lex and Yacc inputs for the two compilers and a
description of the simplification rules used in the normal-form
algorithm.},
     file = "TPVS-progress.ps.Z",
         year = 1994
}


